<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_listado_invitado"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.mikel.gestordepartidos.mostrar_partido"
    android:background="@drawable/fondo3"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EJEMPLO"
        android:id="@+id/mostrar_partido_equipo1"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="72dp"
        android:layout_marginLeft="83dp"
        android:layout_marginStart="83dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/mostrar_partido_hora"
        android:text="10:00"
        android:layout_marginTop="22dp"
        android:textColor="@color/colorPrimary"
        android:layout_marginLeft="114dp"
        android:layout_marginStart="114dp"
        android:layout_below="@+id/mostrar_partido_equipo1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
